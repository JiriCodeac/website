{% set pageDate = '20. 4. 2021' %}
{% set client = clients[pageDate] %}
{% set pageTitle = client.name %}
{% set pageDescription = 'I have learned a lot during the cooparation with eScootie. In the end we came up with the great logo that exceeded our expectations.' %}
{% set pageImage = client.image + '-md.jpg' %}

{% include 'components/header.njk' %}

<div class="content content--light content--beveled" id="about-me">
    <div class="content__inner">
        <h4 class="client-title" id="page-title">Logo</h4>
        <div class="client-body" id="page-body">
            <h3>eScootie</h3>
            <p>
                {{ t('escootie.intro') }}
            </p>
            <p>
               {{ t('escootie.work') }}
            </p>
            <p><a href="https://www.escootie.net/"> www.escootie.net</a></p>
            <img class="img-fluid m-5" src="../images/clients/eScootie-logo.svg" alt="eScootie Logo">
            <p class="text-center">
                <img class="img-fluid d-inline-block d-sm-inline-block d-md-none" src="../images/clients/eScootie-mobile-265x540.png" alt="eScootie Web">
                <img class="img-fluid d-none d-md-inline-block d-xl-none" src="../images/clients/eScootie-mobile-344x700.png" alt="eScootie Web">
                <img class="img-fluid d-none d-xl-inline-block" src="../images/clients/eScootie-mobile-530x1080.png" alt="eScootie Web">
            </p>
        </div>
    </div>
</div>

{% include 'components/footer.njk' %}
