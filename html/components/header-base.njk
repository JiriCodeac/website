	<meta itemprop="image" content="<%= pageUrl %>{{ '/images/sample--logo.svg' }}">
	<link rel="icon" href="/images/favicon.png" type="image/png">

    

    <link rel="stylesheet" href="/css/style-main.min.css?v<%= version %>">
    
